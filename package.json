{
  "name": "travel-together",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others  \"npm run start-dev\" \"npm run watch-client\"",
    "start": "ng serve --port 3000",
    "start:dev": "better-npm-run start:dev",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "betterScripts": {
    "watch-client": {
      "command": "ng serve",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development ",
        "PORT": 30000,
        "APIPORT": 30001
      }
    },
    "start-dev": {
      "command": "supervisor ./server/index.js",
      "env": {
        "NODE_PATH": "./server",
        "NODE_ENV": "development ",
        "PORT": 30000,
        "DEVPORT": 30080,
        "APIPORT": 30081
      }
    },
    "start:dev": {
      "command": "ng serve",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 30000,
        "APIPORT": 30001
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "supervisor": "^0.12.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "better-npm-run": "0.0.15",
    "code-point-at": "^1.1.0",
    "codelyzer": "~2.0.0",
    "concurrent": "^0.3.2",
    "concurrently": "^3.4.0",
    "crypto-js": "^3.1.9-1",
    "events": "^1.1.1",
    "fibers": "^2.0.0",
    "http2": "^3.3.6",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mongodb": "^2.2.28",
    "mysql": "^2.13.0",
    "protractor": "~5.1.0",
    "querystringify": "^1.0.0",
    "superagent": "^3.5.2",
    "svg-captcha": "^1.3.9",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  },
  "main": "index.js",
  "author": "xiaotuni <xiaotuni@gmail.com>"
}
